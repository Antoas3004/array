public class public class Smallest_number {

	public static void main(String[] args) {
		//given array
		int a[] = { 75, 109, 108, 7, 1000 };

		int min = a[0];

		for (int i = 0; i < a.length; i++) {
			if (min > a[i]) {
				min = a[i];
			}
		}
		System.out.println("Smallest number in array = " + min);
	}
 }
