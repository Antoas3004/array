public class SubSetOfArray {

	public static void main(String[] args) {
		int counter = 0;
		int a[] = { 1, 2, 3, 4, 5, 6, 7 };
		int b[] = { 1, 3, 5, 5 };
		for (int i = 0; i < b.length; i++) {
			for (int j = 0; j < a.length; j++) {
				if (b[i] == a[j]) {
					counter = counter + 1;
				}
			}
		}
		System.out.println(counter);
		if (counter == b.length) {
			System.out.println("b Array sub set of Array a");
		} else {
			System.out.println("b array not sub set of Array a");
		}
	}
}
