public class Remove_duplicate {

	public static void main(String[] args) {

		int a[] = { 1, 1, 3, 3, 5, 5, 6, 6, 7 };
		
		//in this type if remove duplicate element and replace it 0

		for (int i = 0; i < a.length; i++) {
			for (int j = i + 1; j < a.length; ++j) {
				if (a[i] == a[j]) {
					a[j] = 0;
				}
			}
		}
		for (int b : a) {
			System.out.print(b + ",");
		}
	}
}
