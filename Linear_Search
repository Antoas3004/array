public class Linear_Search {

	public static void main(String[] args) {

		int key = 50;

		int a[] = {  55, 15, 17, 13, 80,50 };

		for (int i = 0; i < a.length; i++) {
			if (key == a[i]) {
				System.out.println("key found in index");
				break;
			}else {
				System.out.println("no");
			}
		}
	}
}
