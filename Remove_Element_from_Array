public class Remove_Element_from_Array {

	public static void main(String[] args) {
		// Given array
		int a[] = { 1, 2, 3, 4, 5 };
		// remove index
		int remove = 2;
		// for loop
		for (int i = 0; i < a.length; i++) {
			if (i == remove) {
			// continue statement omit that condition
				continue;
			}
			System.out.println(a[i]);
		}
	}
}
